@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Leaderboard";
}

<h2>Leaderboard</h2>

<table>
    <thead>
        <tr>
            <th>Team Name</th>
            <th>Wins</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var entry in Model)
        {
            <tr>
                <td>@entry.TeamName</td>
                <td>@entry.Wins</td>
            </tr>
        }
    </tbody>
</table>
